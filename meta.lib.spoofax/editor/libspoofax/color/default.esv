module libspoofax/color/default

imports

  libspoofax/color/colors

colorer // Default, token-based highlighting

  keyword    : 127 0 85 bold
// Disable color references for ESV baseline support, re-enable after bootstrapping.
//  identifier : default
//  string     : blue
